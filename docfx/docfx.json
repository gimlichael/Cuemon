            "Cuemon.Net/**.csproj",
            "Cuemon.Resilience/**.csproj",
            "Cuemon.Runtime.Caching/**.csproj",
            "Cuemon.Security.Cryptography/**.csproj",
            "Cuemon.Threading/**.csproj",
            "Cuemon.Xml/**.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api/dotnet",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.1"
