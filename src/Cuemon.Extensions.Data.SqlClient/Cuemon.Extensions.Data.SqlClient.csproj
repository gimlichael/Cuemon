<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyOriginatorKeyFile>..\cuemon.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <Product>Cuemon .NET Standard</Product>
    <AssemblyName>Cuemon.Extensions.Data.SqlClient</AssemblyName>
    <AssemblyTitle>Cuemon.Extensions.Data.SqlClient</AssemblyTitle>
    <Description>The Cuemon.Extensions.Data.SqlClient assembly provides a default implementation of a SQL Server .NET Data Provider for Cuemon.Data.</Description>
    <Company>Weubphoria</Company>
    <Authors>Michael Mortensen</Authors>
    <Copyright>Copyright © Weubphoria 2009-2020. All rights reserved.</Copyright>
    <FileVersion>6.0.2020.25</FileVersion>
    <Version>6.0.2020.25</Version>
    <AssemblyVersion>6.0.2020.0</AssemblyVersion>
    <Authors>Michael Mortensen</Authors>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Cuemon.Core\Cuemon.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.0" />
  </ItemGroup>

</Project>