<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyOriginatorKeyFile>..\cuemon.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <Product>Cuemon .NET Standard</Product>
    <AssemblyName>Cuemon.Extensions.DependencyInjection</AssemblyName>
    <AssemblyTitle>Cuemon.Extensions.DependencyInjection</AssemblyTitle>
    <Description>The Cuemon.Extensions.DependencyInjection assembly provides extension methods to the Microsoft.Extensions.DependencyInjection package.</Description>
    <Company>Weubphoria</Company>
    <Authors>Michael Mortensen</Authors>
    <Copyright>Copyright © Weubphoria 2009-2020. All rights reserved.</Copyright>
    <FileVersion>6.0.2020.25</FileVersion>
    <Version>6.0.2020.25</Version>
    <AssemblyVersion>6.0.2020.0</AssemblyVersion>
    <Authors>Michael Mortensen</Authors>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <PropertyGroup>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://www.cuemon.net/</PackageProjectUrl>
    <PackageIconUrl>https://nblcdn.net/themes/cuemon.net/img/core/128x128x.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.1" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="3.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Cuemon.Core\Cuemon.Core.csproj" />
  </ItemGroup>

</Project>